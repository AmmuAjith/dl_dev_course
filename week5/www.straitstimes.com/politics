<script type="text/javascript">
var myPlayer;
var myPlayerer = videojs('myPlayerID');
myPlayerer.ready(function(){
	myPlayer = this;
	myPlayer.catalog.getPlaylist('5167822460001', function(error, playlist){
		myPlayer.catalog.load(playlist);
	});
        myPlayer.on('loadedmetadata', function() {
		var details = {
			name: myPlayer.mediainfo.name,
			description: myPlayer.mediainfo.description
		};
		populateVideoTitle( jQuery(myPlayer.el()).parents(".video-viewbox"), details);
	});
});

function populateVideoTitle(evtObj, details) {
	var $videoViewbox,
		$videoLI, 
		$videoLI_cite_html, 
		$videoLI_p_html;
	
	if (details !== null) {
		//console.log('details', details);
		$videoViewbox = evtObj;
		$videoLI_cite_html = details.name;
		$videoLI_p_html = details.description;
	} else {
		$videoViewbox = jQuery(evtObj.target).parents(".video-viewbox");
		$videoLI = jQuery(evtObj.target).closest("li.vjs-playlist-item");
		$videoLI_cite_html = $videoLI.children("cite").html();
		$videoLI_p_html = $videoLI.children("p").html();
	}

	var videoViewbox_mediaBody = $videoViewbox.children(".media-body");
	videoViewbox_mediaBody.children(".media-heading").children("h3").children(".story-headline").html($videoLI_cite_html);
	videoViewbox_mediaBody.children(".media-text").html($videoLI_p_html);
}

jQuery(".vjs-playlist").on("click", function(e) {
	populateVideoTitle(e,null);
});
</script></div></div></div></div>
